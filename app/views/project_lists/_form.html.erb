<%= form_for(@project_list) do |f| %>
	<%#= debug(@project_list.errors.full_messages) %>
      <ul>
      <% @project_list.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      </ul>
  <% end %>
<div class="form-group row">
	<div class="control-label">
		<%= f.label :title %>
	</div>
	<div class ="col-md-3">
		<%= f.text_field :title, class: "form-control" %>
	</div>
</div>
<div class="form-group row">
	<div>
		<%= f.label :startdate %>
	</div>
	<div>
		<%= f.date_field :startdate  %>
	</div>
</div>
<div class="form-group row">
	<div >
		<%= f.label :enddate %>
	</div>
	<div>
		<%= f.date_field :enddate %>
	</div>
</div>
<div class="form-group row">
	<div >
		<%= f.label :duration %>
	</div>
	<div>
		<%= f.text_field :duration %>
	</div>
</div>
<div class="form-group row">
	<div class="control-label">
		<%= f.label :description %>
	</div>
	<div class="col-md-3">
		<%= f.text_area :description, class: "form-control" %>
	</div>
</div>
<div class="form-group row">
	<div class="control-label">
		<%= f.label :roles %>
	</div>
	<div class="col-md-3">
		<%= f.text_field :roles, class: "form-control" %>
	</div>

</div>
<%= f.submit "CreatProject", class: "btn btn-primary" %>
<% end %>